            android:name=".MainActivity"
            android:exported="false">
            <intent-filter>
                <action android:name="com.example.map_project.MainActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>